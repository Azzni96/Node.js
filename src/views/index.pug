doctype html
html(lang="en")
    head
        meta(charset="UTF-8")
        meta(name="viewport", content="width=device-width, initial-scale=1.0")
        title Document
    body 
        h1= title
        p= message
        each item in exampleData 
            h3 #{item.title}
            p Description: #{item.description}
            p Media ID: #{item.mediaId}